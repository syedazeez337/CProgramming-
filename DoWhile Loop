// Programme to get first 100 natural numbers.

#include <stdio.h>

int main(void)
{
  int i;
  int sum = 0;
  printf("Enter the value of n: ");
  scanf("%d", &i);

  do
    {
      sum = sum + i;
      i--;
    }
    while(i>0);
    printf("sum=%d\n", sum);
} // not working with 100,000 and above

